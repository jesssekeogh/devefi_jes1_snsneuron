      "type": "motoko",
      "gzip": true,
      "declarations": {
        "node_compatibility": true,
        "output": "tests/pic/setup/sns_test_pylon/declarations"
      },
      "post_install": [
        "cp .dfx/ic/canisters/sns_test_pylon/sns_test_pylon.wasm.gz tests/pic/setup/sns_test_pylon"
      ]
